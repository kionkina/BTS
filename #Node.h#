#pragma once

class Node{
 private:
  int val;
  Node *left;
  Node *right;
public:
  Node();
  Node(int val);
  Node(int val, Node* left, Node* right);
  void setChildren(int left_val, int right_val);
  void setLeft(Node *left);
  void setRight(Node *right);
  int getVal();
  Node* getLeft();
  Node* getRight();
};
